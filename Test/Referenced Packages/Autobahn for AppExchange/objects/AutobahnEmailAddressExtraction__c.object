var testmail = new sforce.SObject(&apos;TRC_AB__AutobahnTestMail__c&apos;);
testmail.TRC_AB__ObjectId__c = id;

var callback=
{
onSuccess:function(response)
{
alert(&quot;現時点の抽出件数をメールで送信しました。\n５分以内にログインされているユーザーの登録電子メール宛に送信されます。&quot;);
},
onFailure:function(error)
{
alert(&quot;抽出件数メールの送信登録に失敗しました。\n&quot; + error);
}
};

sforce.connection.create([ testmail ], callback);</url>
